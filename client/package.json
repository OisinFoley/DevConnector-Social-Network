{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "devconnector-types": "^1.0.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "react-moment": "^0.9.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/components/**/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/utils/*.{ts,tsx}",
      "src/App.ts",
      "src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "scripts": {
    "build-css": "node-sass --output-style compressed -x ./src/styles/scss/App.scss ./src/App.css",
    "watch-css": "npm run build-css && node-sass --output-style compressed -x ./src/styles/scss/App.scss ./src/App.css --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test --coverage --watchAll=false --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/mocha": "^7.0.1",
    "@types/react-router-dom": "^5.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "^24.5.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.5.3",
    "ts-jest": "^25.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
