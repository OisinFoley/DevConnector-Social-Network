{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-moment": "^0.9.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/*.{js,jsx}",
      "src/components/**/*.{js,jsx}",
      "src/reducers/*.{js,jsx}",
      "src/utils/*.{js,jsx}",
      "src/App.js",
      "src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "scripts": {
    "build-css": "node-sass -x ./src/styles/scss/App.scss ./src/App.css",
    "watch-css": "npm run build-css && node-sass -x ./src/styles/scss/App.scss ./src/App.css --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.5.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.5.3"
  }
}
